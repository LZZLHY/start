{
  "name": "start-backend",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "predev": "npm run db:prepare",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "prestart": "npm run db:prepare",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:prepare": "node scripts/load-env-and-run.js migrate && node scripts/load-env-and-run.js generate && node scripts/load-env-and-run.js seed",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.10.8",
    "fast-check": "^4.5.3",
    "tsx": "^4.20.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
